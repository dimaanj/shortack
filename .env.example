FIREBASE_PROJECT_ID=
GOOGLE_APPLICATION_CREDENTIALS=

# Web Push (Phase 3). Generate with: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
